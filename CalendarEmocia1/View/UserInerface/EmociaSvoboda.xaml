<UserControl x:Class="CalendarEmocia1.View.UserInerface.EmociaSvoboda"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CalendarEmocia1.View.UserInerface"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="170">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="90"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Image Source="{Binding svobodaKartinka, Mode=TwoWay}" x:Name="KnopkaDate"  Grid.RowSpan="5" Width="65" Height="100" />
        <TextBlock Text="{Binding Emociii,Mode=TwoWay}" x:Name="Emociya"  Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="25"/>
    </Grid>
</UserControl>
